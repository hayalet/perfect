{
  "name": "Perfect",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "*://*/*"
  ]
}